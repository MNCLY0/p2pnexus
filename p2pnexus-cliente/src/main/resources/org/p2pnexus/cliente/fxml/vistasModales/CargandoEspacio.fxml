<StackPane fx:id="stackPanePrincipal" prefHeight="700.0" prefWidth="614.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.p2pnexus.cliente.controladores.vistasModales.ControladorCargarEspacio">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <VBox>
         <children>
            <VBox>
               <children>
                  <Label fx:id="labelNombreEspacio" styleClass="title-2" text="Nombre espacio" />
                  <Label fx:id="labelRuta" styleClass="title-4" text="ruta espacio" textOverrun="CENTER_WORD_ELLIPSIS" />
               </children>
            </VBox>
            <VBox alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
               <children>
                  <Label text="Intentado conectar..." />
                  <ProgressBar prefWidth="200.0" />
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</StackPane>
